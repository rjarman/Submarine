<?xml version='1.0' encoding='utf-8'?>
<widget id="com.heaplinker.submarine" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Submarine</name>
    <description>
        It's a software part of SSD(Submersible Surveillance Drone) project.
    </description>
    <author email="armanrafsunjany@gmail.com" href="http://heaplinker.com">
        Rafsun Jany Arman
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
